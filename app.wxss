page{
  background-color: #f6f8fa;
  font-size: 28rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.clearfix::after{
    content:'';
    display: block;
    height: 0;
    clear: both;
}
.clearfix{
    zoom: 1;
} 
