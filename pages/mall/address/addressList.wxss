page {
  height: 100%;
}
.contain {
  width: 100%;
  height: 100%;
  background:rgba(246,248,250,1);
}
.list {
  margin: 0 29rpx;
}
.addr {
  display: flex;
  align-items: center;
  width:690rpx;
  min-height:143rpx;
  margin-top: 20rpx;
  padding: 32rpx 24rpx 20rpx 24rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
}
.info {
  box-sizing: border-box;
  width: 528rpx;
  float: left;
  padding-right: 19rpx; 
  border-right: 1rpx solid #EBEBEB;
}
.msg {
  margin-bottom: 10rpx;
}
.info  .name {
  font-size:32rpx;
  font-weight:bold;
}
.info .phone {
  font-size:28rpx;
  font-weight:500;
}
.default {
  display: inline-block;
  width:72rpx;
  height:40rpx;
  line-height: 40rpx;
  font-size:24rpx;
  text-align: center;
  margin-right: 9rpx;
  color:rgba(255,175,65,1);
  background:rgba(255,175,65,.1);
  border-radius:8rpx;
}
.detail { 
  height:40rpx;
  line-height: 55rpx;
  font-size:28rpx;
  color:rgba(102,102,102,1);
}
.valid {
  width:1rpx;
  height:100rpx;
  margin-right:18rpx;
  float: left;
}
.edit {
  display: flex;
  align-items: center;
  float: left;
  font-size:24rpx;
  color:rgba(174,175,178,1);
}
.edit-style {
  font-size: 34rpx;
}
.clearfix:after { 
  content: "."; 
  display: block; 
  clear: both; 
  visibility: hidden;
  height: 0;
  font-size:0;  
  } 
.clearfix { zoom:1;}
.add {
  position: fixed;
  bottom: 32rpx;
  left: 50%;
  transform: translateX(-50%);
  width:300rpx;
  height:80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  background:linear-gradient(90deg,rgba(255,175,65,1),rgba(255,132,65,1));
  border-radius:40rpx;
}