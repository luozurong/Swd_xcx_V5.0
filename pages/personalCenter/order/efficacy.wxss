.shopCart-cancel{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
}
.shopCart-cancel>view{
  position: absolute;
  width: 100%;
  height: 788rpx;
  bottom: 0;
  left: 0;
  z-index: 2002;
  background: #fff;
  border-radius: 32rpx 32rpx 0rpx 0rpx; 
}
.shopCart-cancel-word{
  text-align: center;
  font-size: 32rpx;
  color: #333;
  margin-top: 29rpx;
}
.shopCart-cancel-num{
  text-align: center;
  font-size: 28rpx;
  color: #666; 
  margin-top: 13rpx;
}
.shopCart-cancel-wrap{
  display: flex;
  flex-wrap: wrap;
  padding-left: 30rpx;
  margin-top: 12rpx;
  height: 542rpx;
  overflow-y: auto; 
}
.shopCart-cancel-item{
  width: 148rpx;
  height: 148rpx;
  position: relative;
  border-radius: 8rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.shopCart-cancel-item image{
  width: 148rpx;
  height: 148rpx;
  border-radius: 8rpx;
}
.shopCart-cancel-item text{
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 2003;
  width: 100%;
  text-align: center;
  line-height: 42rpx;
  font-size: 24rpx;
  background: rgba(0,0,0,0.6);
  border-radius: 0 0 8rpx 8rpx;
  color: #fff;
}
.shopCart-cancel-btn{
  display: flex;
  justify-items: center;
  height: 112rpx;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 2003;
  width: 100%;
}
.shopCart-cancel-btn text:first-child{
  width: 200rpx;
	height: 78rpx;
	border: 1rpx solid #ff8441;
  line-height: 78rpx;
  text-align: center;
  color: #ff8441;
  border-radius: 40rpx;
  margin: 0 20rpx;
}
.shopCart-cancel-btn text:last-child{
  width: 200rpx;
	height: 80rpx;
	background-image: linear-gradient(90deg, 
		#ffaf41 0%, 
		#ff8441 100%), 
	linear-gradient(
		#f89283, 
		#f89283);
	background-blend-mode: normal, 
		normal;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  color: #fff;
  margin: 0 20rpx;
}
.shopCart-cancel-view{
  height: 112rpx;
}