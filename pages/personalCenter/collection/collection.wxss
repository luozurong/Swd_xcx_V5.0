/* pages/personalCenter/collection/collection.wxss */
image {
  width: 100%;
  height: 100%;
}
.collection {
  border-top: 1px solid #ebebeb;
}
.collection .collection_li {
  display: flex;
  justify-content: space-between;
  padding: 32rpx 25rpx 32rpx 28rpx;
  border-bottom: 1px solid #ebebeb;
  background: #ffffff;
}
.collection .collection_li:last-of-type {
  border-bottom: none;
}
.collection .collection_li .collection_li_right {
  width: 200rpx;
	height: 148rpx;
  margin-left: 30rpx;
	background-color: #c7c7c7;
	border-radius: 12rpx;
  overflow: hidden;
}
.collection .collection_li .collection_li_left {
  width: 466rpx;
  font-size: 14px;
	color: #333333;
}
.collection .collection_li .collection_li_left .title {
  height: 114rpx;
  font-weight: bold;
}
.collection .collection_li .collection_li_left .msg {
  display: flex;
  justify-content: space-between;
  font-size: 12px;
	color: #333333;
}
.collection .collection_li .collection_li_left .msg .msg_right {
  color: #a8a8a8;
}
.collection .collection_li_left .msg .msg_right text {
  margin-left: 20rpx;
}
