page {
  height: 100%;
}
.contain {
  width: 100%;
  height: 100%;
  overflow: scroll;
  background:rgba(246,248,250,1);
  border-top: 1px solid rgba(247, 245, 245, 0.5);
}
.section {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width:750rpx;
  padding: 0 31rpx;
  box-sizing: border-box;
  height:113rpx;
  background:rgba(255,255,255,1);
}
.location {
  display: flex;
  font-size: 28rpx;
}
.location-icon {
  margin-top: 6rpx;
  margin-right: 25rpx;
  width:26rpx;
  height:28rpx;
}
.van-icon {
  color: #AEAFB2;
  font-size: 28rpx;
  top: 4rpx;
}
.out {
  margin-top: 56rpx;
  justify-content: center;
  font-size:28rpx;
  color:rgba(255,110,110,1);
  opacity: 1;
}
  