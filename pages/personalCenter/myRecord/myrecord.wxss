page {
  height: 100%;
}
.contain {
  width: 100%;
  height: 100%;
  overflow: scroll;
  background:rgba(246,248,250,1);
}
.item {
  margin: 20rpx 30rpx;
  padding: 40rpx 20rpx 45rpx;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
}
.user {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.user-name {
  display: flex;
  align-items: center;
  font-size:28rpx;
  font-weight:600;
}
.user-name text:nth-child(2) {
  display: inline-block;
  width:112rpx;
  height:32rpx;
  margin-left: 13rpx;
  line-height: 29rpx;
  text-align: center;
  border:1rpx solid #3ac756;
  border-radius:8rpx;
  font-size:20rpx;
  color:#3ac756;
  font-weight: 400;
}
.user-time {
  font-size:24rpx;
  color:rgba(174,175,178,1);
}
.diagnose {
  margin-top: 19rpx;
  font-size:28rpx;
  color: #666666;
  color:rgba(102,102,102,1);
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;    
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-wrap:break-word;
  word-break:break-all;
}