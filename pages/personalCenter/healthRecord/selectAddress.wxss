/* pages/personalCenter/healthRecord/selectAddress.wxss */
.selectAddress {
  position: relative;
}
.selectAddress .title {
  position: fixed;
  top: 0;
  z-index: 100;
  width: 720rpx;
  padding: 10px 30rpx;
  font-size: 14px;
  background: #ffffff;
}
.selectAddress_li {
  position: relative;
  padding-top: 44px;
  padding-bottom: 150rpx;
}
.selectAddress_li .btn {
  position: fixed;
  bottom: 35rpx;
  left: 75rpx;
  z-index: 99;
  width: 600rpx;
	height: 80rpx;
  line-height: 80rpx;
  font-size: 14px;
	color: #ffffff;
	background-image: linear-gradient(90deg, 
		#73e539 0%, 
		#3ac756 100%), 
	linear-gradient(
		#d2d2d4, 
		#d2d2d4);
	background-blend-mode: normal, 
		normal;
	border-radius: 40rpx;
}