page {
  height: 100%;
}
.contain {
  width: 100%;
  height: 100%;
  /* overflow: scroll; */
  background:rgba(246,248,250,1);
}
.list {
  padding-top: 31rpx;
}
.item {
  margin: 0 30rpx 56rpx;
}
.item-time {
  margin-bottom: 13rpx;
  text-align: center;
  font-size:24rpx;
  color:rgba(174,175,178,1);
}
.item-msg {
  width:690rpx;
  padding: 26rpx 26rpx 30rpx 26rpx;
  box-sizing: border-box;
  font-size:28rpx;
  font-weight:500;
  line-height:48rpx;
  color:rgba(51,51,51,1);
  background:rgba(255,255,255,1);
  border-radius:8rpx;
}
.check-detail {
  display: flex;
  align-items: center;
  margin-top: 25rpx;
  margin-bottom: -20rpx;
  font-size:28rpx;
  color:rgba(58,199,86,1);
}
.check-icon {
  margin-left: 6rpx;
  width:30rpx;
  height:30rpx;
}
